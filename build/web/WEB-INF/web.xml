<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>log</servlet-name>
        <servlet-class>log</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logA</servlet-name>
        <servlet-class>logA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginDAO_A</servlet-name>
        <servlet-class>loginDAO_A</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Continue</servlet-name>
        <servlet-class>Continue</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Kotlin</servlet-name>
        <servlet-class>Kotlin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signout</servlet-name>
        <servlet-class>signout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Call</servlet-name>
        <servlet-class>Call</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>log</servlet-name>
        <url-pattern>/log</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logA</servlet-name>
        <url-pattern>/logA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginDAO_A</servlet-name>
        <url-pattern>/loginDAO_A</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Continue</servlet-name>
        <url-pattern>/Continue</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Kotlin</servlet-name>
        <url-pattern>/Kotlin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signout</servlet-name>
        <url-pattern>/signout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Call</servlet-name>
        <url-pattern>/Call</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>start.jsp</welcome-file>
    </welcome-file-list>
</web-app>
